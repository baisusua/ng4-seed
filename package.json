{
  "name": "angular2-webpack-starter",
  "version": "6.0.0",
  "description": "An Angular 2 Webpack Starter kit featuring Angular 2 (Router, Http, Forms, Services, Tests, E2E, Coverage), Karma, Protractor, Jasmine, Istanbul, TypeScript, and Webpack by AngularClass",
  "keywords": [
    "angular",
    "angular2",
    "webpack",
    "typescript"
  ],
  "author": "Patrick Stapleton <patrick@angularclass.com>",
  "homepage": "https://github.com/angularclass/angular2-webpack-starter",
  "license": "MIT",
  "scripts": {
    "webdriver": "webdriver-manager update",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "test": "karma start",
    "node": "node",
    "rimraf": "rimraf",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "protractor": "protractor",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "npm run webpack-dev-server -- --config task/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "start": "npm run server:dev:hmr",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "dependencies": {
    "@angular/animations": "^5.0.1",
    "@angular/common": "^5.0.1",
    "@angular/compiler": "^5.0.1",
    "@angular/core": "^5.0.1",
    "@angular/forms": "^5.0.1",
    "@angular/http": "^5.0.1",
    "@angular/platform-browser": "^5.0.1",
    "@angular/platform-browser-dynamic": "^5.0.1",
    "@angular/platform-server": "^5.0.1",
    "@angular/router": "^5.0.1",
    "@angularclass/conventions-loader": "^1.0.13",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "child_process": "^1.0.2",
    "colors": "^1.1.2",
    "core-js": "^2.5.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-qiniu": "^0.2.4",
    "gulp-replace": "^0.6.1",
    "gulp-sequence": "^0.4.6",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.5.1",
    "http-server": "^0.10.0",
    "ie-shim": "^0.1.0",
    "jpush-ui": "^0.1.18",
    "last": "^1.1.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "save-dev": "^2.0.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^5.0.1",
    "@ngtools/webpack": "^1.8.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.3",
    "@types/node": "^8.0.50",
    "@types/source-map": "^0.5.2",
    "@types/uglify-js": "^2.6.29",
    "@types/webpack": "^3.8.0",
    "add-asset-html-webpack-plugin": "^2.1.2",
